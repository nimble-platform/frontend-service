{
  "name": "angular-nimble",
  "version": "1.0.0",
  "description": "NIMBLE front-end",
  "scripts": {
    "build": "tsc -p src/",
    "build:watch": "tsc -p src/ -w",
    "build:e2e": "tsc -p e2e/",
    "serve": "lite-server -c=bs-config.json",
    "serve:e2e": "lite-server -c=bs-config.e2e.json",
    "prestart": "npm run build",
    "start": "concurrently \"npm run build:watch\" \"npm run serve\"",
    "pree2e": "npm run build:e2e",
    "e2e": "concurrently \"npm run serve:e2e\" \"npm run protractor\" --kill-others --success first",
    "preprotractor": "webdriver-manager update",
    "protractor": "protractor protractor.config.js",
    "pretest": "npm run build",
    "test": "concurrently \"npm run build:watch\" \"karma start karma.conf.js\"",
    "pretest:once": "npm run build",
    "test:once": "karma start karma.conf.js --single-run",
    "lint": "tslint ./src/**/*.ts -t verbose",
    "i18n": "./node_modules/.bin/ng-xi18n -p ./src/tsconfig.json"
  },
  "keywords": [],
  "author": "",
  "license": "Apache-2.0",
  "dependencies": {
    "@angular/animations": "^5.2.x",
    "@angular/common": "^5.2.x",
    "@angular/compiler": "^5.2.x",
    "@angular/compiler-cli": "^5.2.x",
    "@angular/core": "^5.2.x",
    "@angular/forms": "^5.2.x",
    "@angular/http": "^5.2.x",
    "@angular/platform-browser": "^5.2.x",
    "@angular/platform-browser-dynamic": "^5.2.x",
    "@angular/platform-server": "^5.2.x",
    "@angular/router": "^5.2.x",
    "@angular/upgrade": "^5.2.x",
    "@ng-bootstrap/ng-bootstrap": "^1.1.x",
    "core-js": "^2.5.6",
    "d3": "^5.4.x",
    "file-saver": "^1.3.x",
    "jshint": "^2.9.x",
    "moment": "^2.22.x",
    "ng2-cookies": "^1.0.x",
    "rxjs": "^5.5.x",
    "systemjs": "^0.20.x",
    "underscore": "^1.9.0",
    "zone.js": "^0.8.x"
  },
  "devDependencies": {
    "@types/angular": "^1.6.45",
    "@types/angular-animate": "^1.5.x",
    "@types/angular-cookies": "^1.4.x",
    "@types/angular-mocks": "^1.5.x",
    "@types/angular-resource": "^1.5.x",
    "@types/angular-route": "^1.3.x",
    "@types/angular-sanitize": "^1.3.x",
    "@types/d3": "^5.0.0",
    "@types/file-saver": "^1.3.x",
    "@types/jasmine": "^2.8.7",
    "@types/node": "^9.6.16",
    "@types/underscore": "^1.8.x",
    "babel-cli": "^6.26.x",
    "babel-preset-angular2": "^0.0.x",
    "babel-preset-env": "^1.7.0",
    "canonical-path": "^0.0.x",
    "concurrently": "^3.5.x",
    "http-server": "^0.11.x",
    "jasmine": "^3.1.x",
    "jasmine-core": "^3.1.x",
    "karma": "^1.7.x",
    "karma-chrome-launcher": "^2.2.x",
    "karma-cli": "^1.0.x",
    "karma-jasmine": "^1.1.2",
    "karma-jasmine-html-reporter": "^1.1.0",
    "karma-phantomjs-launcher": "^1.0.x",
    "lite-server": "^2.3.x",
    "lodash": "^4.17.10",
    "phantomjs-prebuilt": "^2.1.x",
    "protractor": "^5.3.2",
    "rollup": "^0.57.x",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-node-resolve": "^3.3.x",
    "rollup-plugin-uglify": "^3.0.x",
    "source-map-explorer": "^1.5.x",
    "tslint": "^5.10.0",
    "typescript": "<2.7",
    "typings": "^2.1.x"
  },
  "repository": {}
}